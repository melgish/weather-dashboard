{
  "name": "@njf/weather-dashboard",
  "version": "0.1.0",
  "description": "stuff",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage",
    "start": "run-p security-check lint:watch open:src test:watch",
    "lint": "esw --color tasks buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "test:demo": "mocha --reporter progress buildScripts/testSetup \"client/**/*.spec.js\"",
    "test": "karma start --browsers=jsdom --single-run",
    "test:watch": "karma start --browsers=jsdom",
    "security-check": "nsp check",
    "open:src": "babel-node buildScripts/srcServer",
    "clean:dist": "rimraf ./dist",
    "prebuild": "run-p clean:dist test lint",
    "build": "babel-node buildScripts/build",
    "postbuild": "babel-node buildScripts/distServer"
  },
  "author": "Nicholas J. Fiorello Jr.",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "compression": "^1.6.2",
    "connect-history-api-fallback": "^1.3.0",
    "express": "^4.15.3",
    "morgan": "^1.8.2",
    "request-promise": "^4.2.1"
  },
  "babel": {
    "babelrc": false,
    "presets": [
      [
        "env",
        {
          "targets": "node"
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es6": true,
      "jasmine": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "rules": {
      "no-console": 1
    }
  },
  "devDependencies": {
    "@uirouter/angularjs": "^1.0.3",
    "angular": "^1.6.4",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-env": "^1.5.1",
    "babel-register": "^6.24.1",
    "bootstrap": "^4.0.0-alpha.6",
    "chai": "^4.0.2",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "eslint": "^4.1.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-watch": "^3.1.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "gulp4": "^4.0.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.6.4",
    "json-server": "^0.10.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-jsdom-launcher": "^6.1.2",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.2",
    "ng-annotate-loader": "^0.6.1",
    "ng-cache-loader": "0.0.25",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "open": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "pug": "^2.0.0-rc.2",
    "pug-html-loader": "^1.1.5",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melgish/weather-dashboard.git"
  }
}
